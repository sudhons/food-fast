{
  "setupFiles": ["<rootDir>/enzyme.config.js"],
  "clearMocks": true,
  "moduleNameMapper": {
    "\\.(css|scss|png|jpg|jpeg|gif)$": "<rootDir>/src/__mocks__/styleMock.js"
  },
  "collectCoverageFrom": [
    "src/**/*.{js,jsx}",
    "!public/*.{js}",
    "!src/index.js",
    "!src/store.js",
    "!src/reducers/index.js"
  ]
 }
